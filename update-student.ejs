        <h2 class="text-white">Update Details</h2>
        <p class="text-white">We would love to hear your thoughts, concerns or problem with anything so we can improve!</p>
        <hr>
		<form action="" method = "POST">
            <input type="hidden" name="id" value = "<%= student[0].id %>">
		    <!-- <h4 >Feedback type</h4> -->
		    <!-- <div class="row ">
		      <div class="col-md-4 ">
		    	<input type= "radio" name="feed" class="pointer"  >&nbsp;&nbsp;<label>Comments</label>    
		      </div> -->
		      <!-- <div class="col-md-4">
		    	<input type="radio" name="feed" >&nbsp;&nbsp;<label>Bug Reports</label>    
		      </div>       -->
		      <!-- <div class="col-md-4">
		    	<input type="radio" name="feed" >&nbsp;&nbsp;<label>Questions</label>    
		      </div> -->
		    </div>

		    <div class="form-group mt-3 mb-3" >
		    	<label class="form-label">Full Name:</label>
		    	<!-- <input type="text" class="form-control " required=""> -->
                <input type="text" name = "name" value = "<%= student[0].name %>" placeholder="Enter Name"  ><br> <br>
		  	</div>
		  	<div class="form-group mb-2">
		    	<label  class="form-label">Enroll:</label>
		    	<!-- <input type="Email" class="form-control" required=""> -->
                <input type="text" name = "enroll" value = "<%= student[0].enroll %>" placeholder="Enter Enrollment" ><br> <br>
    
		  	</div>
		    <div class="form-group mb-2">
		    	<label  class="form-label">Contact:</label>
		    	<!-- <textarea rows="4" class="form-control" required=""></textarea> -->
                <input type="text" name = "contact" value = "<%= student[0].contact %>" placeholder="Enter contact" ><br> <br>
		  	</div>
              <div class="form-group mb-2">
		    	<label  class="form-label">Gender:</label>
		    	<!-- <textarea rows="4" class="form-control" required=""></textarea> -->
                <input type="text" name = "gender" value = "<%= student[0].gender %>" placeholder="Gender" ><br> <br>
		  	</div>
              <div class="form-group mb-2">
		    	<label  class="form-label">Places:</label>
		    	<!-- <textarea rows="4" class="form-control" required=""></textarea> -->
                <input type="text" name = "city" value = "<%= student[0].city %>" placeholder="Enter city" ><br> <br>
		  	</div>
              <div class="form-group mb-2">
